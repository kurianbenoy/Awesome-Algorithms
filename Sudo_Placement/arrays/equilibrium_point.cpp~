#include<bits/stdc++.h>
using namespace std;

int main()
{
  int T, n,count=0;
  cin>>T;
  while(T--)
    {
      vector<int> A;
      int t;
      cin>>n;
      for(int i=0;i<n;i++){
	cin>>t;
	A.push_back(t);
      }
		
	// 1 2 3 4 -1
	for(int i=0;i<n;i++)
	{
		int s1=0, s2 = 0;

		for(int j=i;j<n;j++)
			s1 += A[j];

		for(int k=0;k<=i;k++)
			s2 += A[k];

		if(s1 == s2)
		{
			cout<<i<<endl;
		}
		cout<<s1<<"s2:"<<s2<<endl;
		if(count ==0)
			cout<<-1<<endl;
	}
    }
	return 0;
}	  
